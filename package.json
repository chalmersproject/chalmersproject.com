{
  "name": "chalmersproject.com",
  "version": "0.1.0",
  "private": true,
  "license": "",
  "sideEffects": false,
  "scripts": {
    "devcontainer": "docker exec -itw /workspace chalmersprojectcom_devcontainer_app_devcontainer_1 zsh",
    "dev": "overmind start -N -f Procfile.dev",
    "dev:rails": "yarn -s start:rails",
    "dev:remix": "remix dev",
    "dev:nginx": "nginx -c config/nginx.dev.conf -p \"$PWD\"",
    "build": "run-p -l build:remix build:rails",
    "build:remix": "remix build",
    "build:rails": "bundle exec rails assets:precompile",
    "start": "overmind start -N",
    "start:nginx": "nginx -c config/nginx.conf -p \"$PWD\"",
    "start:rails": "bundle exec rails server",
    "start:remix": "remix-serve build",
    "start:good_job": "bundle exec good_job start",
    "routes": "bundle exec rails routes",
    "console": "bundle exec rails console",
    "restart": "overmind restart",
    "connect": "overmind connect",
    "stop": "overmind stop",
    "quit": "overmind quit",
    "kill": "overmind kill",
    "db": "bundle exec rails db --include-password",
    "db:status": "bundle exec rails db:migrate:status",
    "db:migrate": "bundle exec rails db:migrate",
    "db:migrate:up": "bundle exec rails db:migrate:up",
    "db:migrate:down": "bundle exec rails db:migrate:down",
    "db:migrate:redo": "bundle exec rails db:migrate:redo",
    "db:rollback": "bundle exec rails db:rollback",
    "db:fixtures": "bundle exec rails db:fixtures:load",
    "db:reset": "bundle exec rails db:reset",
    "annotate": "run-p -l annotate:models annotate:routes",
    "annotate:models": "bundle exec rails annotate_models",
    "annotate:routes": "bundle exec rails annotate_routes",
    "gen:gql": "graphql-codegen",
    "gen:srb": "run-s -l gen:srb:gems gen:srb:dsl",
    "gen:srb:gems": "yarn -s gen:srb:gem",
    "gen:srb:gem": "bundle exec tapioca gem",
    "gen:srb:dsl": "bundle exec tapioca dsl",
    "gen:rails": "bundle exec rails generate",
    "fmt": "yarn -s fmt:partial .",
    "fmt:partial": "prettier --write",
    "lint": "run-p -l lint:tsc lint:eslint lint:srb lint:cops",
    "lint:tsc": "tsc -b",
    "lint:tsc:partial": "tsc-files --noEmit",
    "lint:eslint": "yarn -s lint:eslint:partial .",
    "lint:eslint:partial": "eslint --cache --ext .js,.jsx,.ts,.tsx",
    "lint:srb": "bundle exec srb tc",
    "lint:cops": "bundle exec rubocop",
    "lint:cops:partial": "bundle exec rubocop --force-exclusion",
    "fix": "run-s -l fix:eslint fix:cops",
    "fix:eslint": "yarn -s lint:eslint --fix",
    "fix:eslint:partial": "yarn -s lint:eslint:partial --fix",
    "fix:cops": "yarn -s lint:cops --auto-correct-all",
    "fix:cops:partial": "yarn -s lint:cops:partial --auto-correct",
    "gem": "bundle",
    "install:remix": "remix setup node",
    "install:gems": "yarn -s gem install",
    "install:husky": "husky install",
    "prepare": "run-p install:husky install:gems install:remix"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@mantine/core": "^3.6.3",
    "@mantine/dates": "^3.6.3",
    "@mantine/dropzone": "^3.6.3",
    "@mantine/hooks": "^3.6.3",
    "@mantine/modals": "^3.6.3",
    "@mantine/notifications": "^3.6.3",
    "@mantine/rte": "^3.6.3",
    "@mantine/ssr": "^3.6.3",
    "@rails/actioncable": "^7.0.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/server-runtime": "^1.1.3",
    "dayjs": "^1.10.7",
    "graphql": "^16.3.0",
    "graphql-ruby-client": "^1.10.6",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.1.3",
    "tailwindcss": "^3.0.17"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typed-document-node": "^2.2.2",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.9",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@prettier/plugin-ruby": "^2.0.0",
    "@remix-run/dev": "^1.1.3",
    "@remix-run/node": "^1.1.3",
    "@types/lodash": "^4.14.178",
    "@types/rails__actioncable": "^6.1.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/tailwindcss": "^3.0.3",
    "@types/zen-observable": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "postinstall-prepare": "^2.0.0",
    "prettier": "^2.5.1",
    "release-it": "^14.12.4",
    "tsc-files": "^1.1.3",
    "typescript": "^4.1.2"
  }
}
